<footer id="myFooter">
    <div class="container">
        <div class="row">
            <div class="col-sm-9 col-md-9 col-lg-9">
                <h2 class="logo"><a href="#"> Printastigo </a></h2>
            </div>
            <div class="col-sm-3 col-md-3 col-lg-3">
                <div class="social-networks">
                    <a href="#" class="twitter"><i class="fa fa-twitter"></i></a>
                    <a href="#" class="facebook"><i class="fa fa-facebook"></i></a>
                    <a href="#" class="google"><i class="fa fa-google-plus"></i></a>
                </div>
            </div>
        </div>
        <hr>
        <div class="row2">
            <div class="col-sm-3 col-md-3 col-lg-3">
                <h4>About Us</h4>
                <h4>FAQs</h4>
                <h4>Terms & Conditions</h4>
                <h4>Privacy Policy</h4>
                <h4>Refunds & Cancellation</h4>
                <h4>Press Releases</h4>
                <h4>Careers</h4>
                <h4>Contact Us</h4>
                <h4>Blog</h4>
            </div>
            <div class="col-sm-6 col-md-6 col-lg-6">
                <h5>All Categories</h5>
                <ul class="list-inline">
                    {% for product in allProducts.select('category').whereNotNull('category').groupBy('category').get() %}
                        <li class="col-sm-4 col-md-4 col-lg-4"><a href="{{ path_for('category.get', {category: product.category}) }}">{{ product.category }}</a></li>
                    {% endfor %}
                    {% for product in allProducts.select('sub-category').whereNotNull('sub-category').groupBy('sub-category').get() %}
                        <li class="col-sm-4 col-md-4 col-lg-4"><a href="{{ path_for('subCategory.get', {subCategory: product['sub-category']}) }}">{{ product['sub-category'] }}</a></li>
                    {% endfor %}
                    {% for product in allProducts.select('classification').whereNotNull('classification').groupBy('classification').get() %}
                        <li class="col-sm-4 col-md-4 col-lg-4"><a href="{{ path_for('classification.get', {classification: product.classification}) }}">{{ product.classification }}</a></li>
                    {% endfor %}
                    {% for product in allProducts.select('specification').whereNotNull('specification').groupBy('specification').get() %}
                        <li class="col-sm-4 col-md-4 col-lg-4"><a href="{{ path_for('specification.get', {specification: product.specification}) }}">{{ product.specification }}</a></li>
                    {% endfor %}
                </ul>
            </div>
            <div class="col-sm-3 col-md-3 col-lg-3">
                <button type="button" class="btn btn-default">Contact us</button>
            </div>
        </div>
    </div>
    <div class="footer-copyright">
        <p>Â© 2016 Copyright Text </p>
    </div>
</footer>